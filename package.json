{
  "name": "swaddlesforwaddles",
  "version": "1.0.0",
  "description": "Meme Creation with a cause. Create memes of penguins in sweaters with the option to donate to keep penguins safe.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cecileanne/SwaddlesForWaddles.git"
  },
  "keywords": [
    "penguin",
    "meme",
    "creator",
    "fun",
    "activism",
    "donation"
  ],
  "author": "Cecile-Anne Sison Sabrina Rolz Rosalis Freebairn Brittney Galloway",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cecileanne/SwaddlesForWaddles/issues"
  },
  "homepage": "peaceful-wildwood-69341.herokuapp.com",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "jimp": "^0.9.3",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react-scripts": "^3.3.0",
    "sequelize": "^5.15.1"
  }
}
